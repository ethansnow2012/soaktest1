<style>
    :root {
        --webColor1: #a7c5d5;
        --webColor2: #ffdead;
        --webColor_grey1: #404040;
        --webColor_grey2: black;
        --webSP_leftNav: min(220px, 30vw);
        --webSP_md_rightNav: 220px;
        --webSP_header: 50px;
        --webSP_iconSize: 30px;
        --webSP_mainHside: min(15%, 23px);
        --webSP_mainVside: 20px;
        --webFontSize1:30px;
        --webFontSize2:14px;
        --svg-main: var(--webColor_grey1);
    }
    @media(max-width: 767px){
        :root {
            --webSP_md_rightNav: 0;
        }
    }
    * {
        box-sizing: border-box;

    }
    html{
        background-color: azure;;
        webkit-overflow-scrolling: touch;
        overflow-x: hidden;
    }
    body {
        margin: 0;
        /* background: url(); */
        color: var(--webColor_grey2);
        webkit-overflow-scrolling: touch;
        overflow-x: hidden;
        position: relative; /* sadly we need this to prevent odd behavior */
    }
    html.no-scroll, body.no-scroll{
        overflow: hidden;
        height: 100vw;
        max-height: 100vw;
    }
    a{
        text-decoration: none;
    }
    .e-header {
        display: flex;
        height: 50px;
        background: var(--webColor1);
        /* background-color: #dfdfdf; */
        align-items: center;
        position: fixed;
        width: 100%;
        z-index: 50000;
    }

    .e-header-right {
        position: absolute;
        display: flex;
        right: 0;
        margin-left: auto;
        cursor: pointer;
        margin-right: 30px;
        justify-content: flex-end;
    }
    .e-header-right-panel{
        margin-right: 12px;
    }
    .e-header-right>img {
        width: 30px;
        height: 30px;
    }

    .e-header-left {
        position: relative;
        display: flex;
        margin: auto;
        justify-content: center;
        text-decoration: none;
        color: unset;
        width: var(--webSP_leftNav);
        letter-spacing: 1px;
        color: var(--webColor_grey1);
        /* text-decoration: underline; */
        text-underline-position: under;
        font-family: EBGaramond;
        font-size: 23px;
    }
    .e-header-left::before{
        content: "";
        width: 6em;
        height: 1px;
        background: var(--webColor_grey1);
        position: absolute;
        top: 17.5px;
        opacity: 0.2;
    }

    .e-header-login {
        margin: 0 20px;
    }

    .e-header-signup {
        margin: 0 20px;
    }

    .e-dropdown {
        position: absolute;
        top: calc(7px + 100%);
        display: none;
        background: var(--webColor1);
        border: 1px solid white;
        right: 2px;
    }
    .e-dropdown-inner {
        position: relative;
    }
    .e-dropdown-inner::after{
        content: '';
        position: absolute;
        bottom: calc(100% - 4px);
        right: 8px;
        width: 8px;
        height: 8px;
        background: var(--webColor_grey1);
        transform: rotate(45deg);
    }
    .e-dropdown.on {
        display: block;
    }

    .e-dropdown-i {
        width: max-content;
        padding: 3px 15px;
    }

    .e-popup {
        display: none;
        position: fixed;
        background: rgba(210, 210, 210, 0.96);
        width: 100%;
        height: 100vh;
        z-index: 10000;
        top: 100vh;
        left: 0;
        align-items: center;
        justify-content: center;
        z-index: 60000;
        transition: all 0.5s ease-out;
    }
    .e-popup.stage {
        display: flex;
        top: 50vh;
        opacity: 0;
    }
    .e-popup.on {
        display: flex;
        top: 0px;
        opacity: 1;
    }
    .e-popup1 .e-popup-content{
        display: grid;
    }

    .e-popup-contentwrapper {
        height: 100vh;
        overflow: scroll;
        width: 100%;
        padding: 20px 40px;
        flex-direction: column;
        justify-content: center;

    }

    .e-popup-content {
        /* display: flex; */
        flex-direction: column;
        height: 100%;
        position: relative;
    }
    .e-popup-content  *{
        max-width: 100%;
    }
    .e-popup-content-shrink{
        width: max-content;
        margin: auto;
        position: relative;
    }
    .e-popup-content-i{
        width: max-content;
        margin: auto;
        margin-right: 0;
    }
    .e-popup-content-icenter{
        margin-left: 0;
        margin-right: 0;
    }
    .e-catagoryandphone-title{
        font-size: 20px;
        font-weight: bolder;
    }
    .e-popup-content-i + .e-popup-content-i{
        margin-top: 5px;
    }
    .e-catagoryandphone-wrapper + .e-catagoryandphone-wrapper{
        margin-top: 15px;
    }
    .e-body {
        padding-top: 200px;
        min-height: 120vh;
        overflow-x: hidden;
    }
    .e-body-topnav + .e-body-topnav{
        margin-top: 17px;
        border-top: 1px solid #a8c5e691;
        padding-top: 10px;
    }
    .e-body-topnav {
        /* background: #80808087; */
        width: 100%;
        min-height: 20px;
        display: flex;
        flex-wrap: wrap;
        position: relative;
    }
    .e-body-topnav-between{
        position: absolute;
        top:var(---top);
        left:var(---left);
        width: 20px;
        height: 20px;
        background: none !important;
        cursor: pointer;
    }
    .e-body-topnav-between * {
        background: none !important;
    }
    .e-body-topnav-t *{
        background: #80808087;
    }
    .e-body-topnav-t{
        display: block;
        height: 40vmin;
        position: relative;
        /* margin-bottom: 43px; */
    }
    .e-body-topnav-t-square{
        ---deg: 45deg;
        width: 100%;
        height: 100%;
        transform: rotate(var(---deg));
        

    }
    .e-body-topnav-t-i-pos{
        position: absolute;
        top: var(---top);
        left: var(---left);
    }
    .e-body-topnav-t-i-el{
        width: 100%;
        height: 100%;
        background: white ;
        transition: all 3s ease;
    }
    .e-body-topnav-t-i{
        ---width:50px;
        ---height:50px;
        width: var(---width);
        height: var(---height);
        position: relative;
    }
    .e-body-topnav-t-i + .e-body-topnav-t-i{
        margin: 5px;
    }
    .e-body-topnav0{
        background: unset;
    }
    .e-body-topnav0-holder{
        width: 5px;
        height: 5px;
        /* background: #80808087; */
        margin-left: auto;
        position: relative;
    }
    .e-body-topnav0-holder > *{
        /* background: #80808087; */
    }
    .e-body-topnav2{
        display: flex;
        align-content: baseline;
    }
    
    .e-body-topnav3{
        /* display: none; */
        min-height: 180px;
    }
    .e-body-topnav3-i.depth1::before {
        content: "";
        position: absolute;
        height: 70%;
        width: 6px;
        left: 0;
        top: 15%;
        background: black;
    }
    .e-body-topnav2-inner{
        position: relative;
        display: flex;
        flex-wrap: wrap;
        transition: all 1s ease-in-out;
        left: 0;
        width: 100%;
        flex-direction: column;
        height: calc(180px + 2em);
        overflow-x: scroll;
        padding-bottom: 2em;
    }
    .e-body-topnav2-inner.active{
        /* visibility: hidden; */
    }
    .e-body-topnav2-inner.active::after{
        content: " ← →";
        position: absolute;
        bottom: 0;
        right: 6px;
    }
    .e-body-topnav2-inner.standby{
        position: absolute;
        left: 100%;
    }
    .e-body-topnav2-inner.fadingout{
        position: absolute;
        left: -100%;
        height: 0;
        overflow: hidden;
        height: 100px;
    }
    .e-body-topnav-i{
        background: #a7c5d469;
        width: max-content;
        height: 50px;
        display: flex;
        justify-content: center;
        align-items: center;
        padding: 0 20px;
        margin: 5px;
        position: relative;
        cursor: pointer;
    }
    .e-body-topnav-i.on{
        background: #6c8e37ab;
    }
    .e-body-leftnav {
        width: var(--webSP_leftNav);
        height: calc(100vh - var(--webSP_header));
        position: fixed;
        top: var(--webSP_header);
        padding-top: 20px;
    }

    .e-body-leftnav-i {
        position: relative;
        background: var(--webColor1);
        padding: 10px 16px;
        width: 92%;
        display: flex;
        cursor: pointer;
        color: var(--webColor_grey2);
    }
    .e-body-leftnav-i.depth1::before {
        content: "";
        position: absolute;
        height: 70%;
        width: 6px;
        left: 0;
        top: 15%;
        background: black;
    }
    .e-body-leftnav-i:hover{
        background: var(--webColor2);
    }

    .e-body-leftnav-i+.e-body-leftnav-i {
        margin-top: 15px;
    }
    .e-body-rightnav{
        position: fixed;
        top: var(--webSP_header);
        right: 0;
        width: var(--webSP_md_rightNav);
        padding-top: 20px;
    }
    .e-rightnav{

    }
    .e-rightnav-headimg img{
        width: 100%;
        border: 1px solid black;
    }
    .e-body-maincontent {
        margin-right: var(--webSP_md_Nav);
        margin-left: var(--webSP_leftNav);
        width: calc(100% - calc(var(--webSP_leftNav) + var(--webSP_md_rightNav)));
        padding: var(--webSP_mainVside) var(--webSP_mainHside);
    }

    .e-phone-wrapper{
        padding: 3vmin 3vmin 2vmin 3vmin;
    }
    .e-phone-wrapper img{
        max-width: 100%;
        height: auto;
    }
    .e-phone-wrapper pre{
        position: relative;
        overflow-x: scroll;
        background: #ffffff94;
        padding: 10px;
        border-radius: 8px;
    }
    .e-phone-wrapper code{

    }
    .e-phone-title{
        font-size: var(--webFontSize1);
        color: var(--webColor_grey2);
    }

    .e-phone-itemwrapper {
        border: 1px solid var(--webColor_grey1);
        border-radius: 5px;
        max-width: 760px;
        position: relative;
        margin: auto;
        overflow: hidden;
        background: #ffffff94;
        padding-top: 3px;
        padding-bottom: 3px;
    }

    .e-phone-item {
        padding: 0px 20px;
        cursor: pointer;
        color: black;
        text-decoration: none;
        display: flex;
        color: var(--webColor_grey2);
        position: relative;
    }

    .e-phone-item:hover {
        background: var(--webColor2);
    }

    .e-phone-item+.e-phone-item {
        border-top: 1px solid var(--webColor_grey1);
    }

    .e-phone-item-text1 {
